<robot xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- XACRO-MACRO of STAUBLI RX-160: -->
  <xacro:macro name="rx160" params="bringup robot_ip EE EE_name">

    <!-- STAUBLI RX-160 TRANSMISSION: -->
    <xacro:unless value="${bringup}">  
      <xacro:include filename="$(find ros2srrc_robots)/rx160/urdf/rx160_transmission.xacro" />
      <xacro:rx160_transmission />
    </xacro:unless>

    <!-- STAUBLI RX-160 GAZEBO - ROS2_CONTROL: -->
    <xacro:include filename="$(find ros2srrc_robots)/rx160/urdf/rx160_ros2control.xacro" />
    <xacro:rx160_ros2control 
      bringup="${bringup}"
      robot_ip="${robot_ip}"
      EE="${EE}"
      EE_name="${EE_name}"
    />

    <!-- ***** STAUBLI RX-160 ROBOT .urdf SERIAL CHAIN ***** -->

    <!-- Link list: -->
    <!-- base_link -->
    <link name="base_link">
      <inertial>
        <mass value="23.085357"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.007579 0.000017 0.180670"/>
        <inertia ixx="0.343576" ixy="-0.000062" ixz="-0.002079" iyy="0.336217" iyz="-0.000014" izz="0.334050"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/visual/base_link.dae"/>
        </geometry>
        <material name="base_link_material">
          <color rgba="1.0 0.607843137255 0.0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/collision/base_link.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- link_1 -->
    <link name="link_1">
      <inertial>
        <mass value="45.357763"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.085479 -0.002547 -0.040488"/>
        <inertia ixx="0.839141" ixy="0.022637" ixz="0.162457" iyy="1.030146" iyz="0.027416" izz="1.064698"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/visual/link_1.dae"/>
        </geometry>
        <material name="link_1_material">
          <color rgba="1.0 0.607843137255 0.0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/collision/link_1.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- link_2 -->
    <link name="link_2">
      <inertial>
        <mass value="51.266495"/>
        <origin rpy="0.0 0.0 0.0" xyz="-0.000002 0.264164 0.347704"/>
        <inertia ixx="5.272800" ixy="0.000010" ixz="0.000017" iyy="5.536809" iyz="-0.008261" izz="0.486191"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/visual/link_2.dae"/>
        </geometry>
        <material name="link_2_material">
          <color rgba="1.0 0.607843137255 0.0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/collision/link_2.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- link_3 -->
    <link name="link_3">
      <inertial>
        <mass value="19.754401"/>
        <origin rpy="0.0 0.0 0.0" xyz="-0.000255 0.016478 -0.003819"/>
        <inertia ixx="0.249388" ixy="-0.004901" ixz="0.004825" iyy="0.211780" iyz="0.000574" izz="0.238488"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/visual/link_3.dae"/>
        </geometry>
        <material name="link_3_material">
          <color rgba="1.0 0.607843137255 0.0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/collision/link_3.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- link_4 -->
    <link name="link_4">
      <inertial>
        <mass value="15.287896"/>
        <origin rpy="0.0 0.0 0.0" xyz="-0.015515 0.000164 0.340348"/>
        <inertia ixx="0.331593" ixy="0.000055" ixz="-0.010688" iyy="0.320895" iyz="0.000058" izz="0.086557"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/visual/link_4.dae"/>
        </geometry>
        <material name="link_4_material">
          <color rgba="1.0 0.607843137255 0.0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/collision/link_4.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- link_5 -->
    <link name="link_5">
      <inertial>
        <mass value="0.548088"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.000000 -0.000347 0.023671"/>
        <inertia ixx="0.000876" ixy="0.000000" ixz="0.000000" iyy="0.000889" iyz="0.000005" izz="0.000412"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/visual/link_5.dae"/>
        </geometry>
        <material name="link_5_material">
          <color rgba="0.529411764706 0.521568627451 0.505882352941 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/collision/link_5.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- link_6 -->
    <link name="link_6">
      <inertial>
        <mass value="0.038484"/>
        <origin rpy="0.0 0.0 0.0" xyz="-0.000245 0.000000 -0.007626"/>
        <inertia ixx="0.000011" ixy="0.000000" ixz="0.000000" iyy="0.000011" iyz="0.000000" izz="0.000021"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/visual/link_6.dae"/>
        </geometry>
        <material name="link_6_material">
          <color rgba="0.529411764706 0.521568627451 0.505882352941 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2srrc_robots)/rx160/meshes/collision/link_6.stl"/>
        </geometry>
      </collision>
    </link>
    <!-- end of link list -->

    <!-- Joint list: -->
    <joint name="joint_1" type="revolute">
      <origin rpy="0 0 0" xyz="0 0 0.55"/>
      <parent link="base_link"/>
      <child link="link_1"/>
      <axis xyz="0 0 1"/>
      <limit effort="692.0" lower="-2.967060" upper="2.967060" velocity="2.88"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>
    <joint name="joint_2" type="revolute">
      <origin rpy="0 0 0" xyz="0.15 0 0"/>
      <parent link="link_1"/>
      <child link="link_2"/>
      <axis xyz="0 1 0"/>
      <limit effort="574.0" lower="-2.4" upper="2.4" velocity="2.62"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>
    <joint name="joint_3" type="revolute">
      <origin rpy="0 0 0" xyz="0 0 0.825"/>
      <parent link="link_2"/>
      <child link="link_3"/>
      <axis xyz="0 1 0"/>
      <limit effort="242.0" lower="-2.62" upper="2.62" velocity="3.32"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>
    <joint name="joint_4" type="revolute">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="link_3"/>
      <child link="link_4"/>
      <axis xyz="0 0 1"/>
      <limit effort="58.0" lower="-4.71" upper="4.71" velocity="5.15"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>
    <joint name="joint_5" type="revolute">
      <origin rpy="0 0 0" xyz="0 0 0.625"/>
      <parent link="link_4"/>
      <child link="link_5"/>
      <axis xyz="0 1 0"/>
      <limit effort="29.0" lower="-1.83" upper="2.09" velocity="4.54"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>
    <joint name="joint_6" type="revolute">
      <origin rpy="0 0 0" xyz="0 0 0.11"/>
      <parent link="link_5"/>
      <child link="link_6"/>
      <axis xyz="0 0 1"/>
      <limit effort="29.0" lower="-4.71" upper="4.71" velocity="7.68"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS-Industrial 'base' frame: base_link to ABB World Coordinates transform -->
    <link name="base" />
    <joint name="base_link-base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="base_link"/>
      <child link="base"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="flange"/>
    <joint type="fixed" name="joint_6-flange">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="link_6"/>
      <child link="flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="tool0" />
    <joint name="link_6-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="0 ${radians(90)} 0" />
      <parent link="flange" />
      <child link="tool0" />
    </joint>

  </xacro:macro>
</robot>